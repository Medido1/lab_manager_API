generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id Int @id @default(autoincrement())
  username String @unique
  password String
  approved Boolean @default(false)
  role Role @default(USER)
}

enum Role  {
  USER
  ADMIN
}

model Session {
  id String @id
  sid String @unique
  data String 
  expiresAt DateTime
}